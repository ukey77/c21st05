{
  "compilerOptions": {
    "target": "es2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": false,
    // "noEmit": true, // 결과물 파일 생성을 방지
    "noEmitOnError": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "**/*.spec.ts", "**/*.js"]
}
